      {
        "pcfMicroServiceHost": "$ULAK_PCF_SMS",
        "pcfMicroServiceName": "npcf-smpolicycontrol",
        "pcfMicroServicePort": 7004
      },
      {
        "pcfMicroServiceHost": "$ULAK_PCF_AMS",
        "pcfMicroServiceName": "npcf-am-policy-control",
        "pcfMicroServicePort": 7001
      }
    ]
  },
  "SchedulerTimers": {
    "DailySchedulerTimer": 60,
    "WeeklySchedulerTimer": 70,
    "MonthlySchedulerTimer": 80,
    "YearlySchedulerTimer": 90,
    "OneTimeSchedulerTimer": 10
  }
