{
  "ServiceSettings": {
    "ServiceInstanceID": "$PCF_OMS_SERVICE_INSTANCE_ID",
    "Versions": [
      {
        "APIVersionInUri": "v1",
        "APIFullVersion": "1.0.0.alpha-1",
        "Expiry": "2025-Feb-15T10:43:23Z"
      }
    ],
    "IPEndPoints": [
      {
        "IPv4Address": "$ULAK_PCF_OMS",
        "IPv6Address": "",
        "Port": 7052,
        "Transport": "TCP"
      }
    ],
    "Scheme": "http",
    "SSLCertificateKeyFile": "certificate/localhost.key",
    "SSLCertificateFile": "certificate/localhost.crt",
    "accessTokenStatus": false,
    "SaveMetricsInDB": false,
    "MetricExposureHost": "",
    "MetricExposurePort": 7060,
    "K8SActive": false,
    "LogDirectory": "/var/log/cinar/pcf/oms/",
    "DisplayLog": true,
    "CreateLogFile": true
